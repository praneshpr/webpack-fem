{
  "name": "webpack-workshop-2018",
  "version": "1.0.0",
  "description": "Learning resources for the webpack academy workshop series for 2018",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "prod:custom": "yarn prod -- --env.presets custom",
    "prod:compress": "yarn prod -- --env.presets compress",
    "prod:analyze": "yarn prod -- --env.presets analyze",
    "prod": "yarn webpack -- --env.mode production",
    "dev": "yarn webpack-dev-server -- --env.mode development --hot",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js --env.mode production",
    "debug:custom": "yarn debug -- --env.presets custom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLarkInn/webpack-workshop-2018.git"
