  "devDependencies": {
    "http-server": "^0.11.1"
  },
  "dependencies": {
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
